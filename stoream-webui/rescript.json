{
  "name": "stoream-webui",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "@glennsl/rescript-fetch"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "jsx": {
    "version": 4
  },
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "bundler",
    "generatedFileExtension": ".gen.tsx"
  }
}